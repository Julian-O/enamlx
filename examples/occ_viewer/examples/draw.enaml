
from occ.part import Part
from occ.draw import Wire, Circle, Ellipse, Line, Point

enamldef Draw(Part):
    name = "Draw Example"

    Wire:
        Circle:
            radius = 10
    Wire:
        Ellipse:
            position = (-10,0,0)
            major_radius = 10
            minor_radius = 4
    Wire:
        #Circle:
        #    radius = 4
        Line:
            Point:
                position = (4,0,0)
            Point:
                position = (10,10,0)
